  </link>

  <joint name="world_tree_fixed_joint" type="fixed">
    <parent link="world" />
    <child link="trees"/>
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
  </joint>

  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="link1"/>
    <origin xyz="0 1 0" rpy="0 0 0" />
    <axis xyz="0 0 0" />
  </joint>


  <joint name="joint1" type="prismatic">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz=".5 -.99 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit effort="1000.0" lower="0" upper="2" velocity="0.2"/>
  </joint>

  <joint name="joint2" type="prismatic">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="-.5 0 0" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit effort="1000.0" lower="0" upper="1" velocity="0.2"/>
  </joint>



  <joint name="joint3" type="prismatic">
    <parent link="link3"/>
    <child link="link_3_4"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="1000.0" lower="0" upper="2" velocity="0.2"/>
  </joint>

  <joint name="joint_3_4" type="fixed">
    <parent link="link_3_4"/>
    <child link="link4"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

  <joint name="joint4" type="revolute">
    <parent link="link4"/>
    <child link="link_4_5"/>
    <origin rpy="0 0 0" xyz="0.1 0. 0.09"/>
    <axis xyz="0 0 1" />
    <limit effort="150.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.14"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <joint name="joint_4_5" type="fixed">
    <parent link="link_4_5"/>
    <child link="link5"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

  <joint name="joint5" type="revolute">
