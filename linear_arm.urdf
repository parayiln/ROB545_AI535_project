  </link>
  <link name="link_4_5">
    <visual>
      <geometry>
        <box size=".14 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz=".06 0 0.01"/>
      <material name="blue"/>
    </visual>
  </link>

  <link name="link5">
    <visual>
      <geometry>
        <cylinder length="0.06" radius="0.02"/>
      </geometry>
      <material name="green"/>
      <origin rpy="0 1.5707 0" xyz="0.15 0. 0.015"/>
    </visual>
  </link>

  <link name="link_5_6">
    <visual>
      <geometry>
        <box size=".14 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz=".06 0 0.01"/>
      <material name="green"/>
