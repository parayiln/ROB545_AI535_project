
<robot name="robot">

  <material name="blue">
  <color rgba="0 0 1 1"/>
  </material>

  <material name="green">
  <color rgba="0 1 0 1"/>
  </material>

  <material name="red">
  <color rgba=".5 0. 0 1"/>
  </material>


  <link name="trees">
    <visual>
      <origin xyz=" 2 -.8 -.7" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://meshes/trees.stl" scale=".001 .001 .001"/>
      </geometry>
      <material name="brown">
        <color rgba=".58 .3 0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz=" 1.3 -.8 -.7" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://meshes/trees.stl" scale=".001 .001 .001"/>
      </geometry>
    </collision>
  </link>


<link name="world"/>
  <link name="link1">
    <visual>
      <geometry>
        <box size="0.03 2.2 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="blue"/>
    </visual>
  </link>

  <link name="link2">
    <visual>
      <geometry>
        <box size="1.2 .03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.03"/>
      <material name="green"/>
    </visual>
  </link>

  <link name="link3">
    <visual>
      <geometry>
        <box size=".03 .03 2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0. 1.045"/>
