    </visual>
  </link>

  <link name="link4">
    <visual>
      <geometry>
        <cylinder length="0.06" radius="0.02"/>
      </geometry>
      <material name="blue"/>
      <origin rpy="0 0 0" xyz="0.1 0. 0.09"/>
